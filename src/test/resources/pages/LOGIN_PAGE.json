{
  "name": "HOME_PAGE",
  "detectors": {
    "android": "HOME_NOTIFICATION_ICON",
    "ios": "HOME_NOTIFICATION_ICON"
  },
  "elements": [
    {
      "name": "HOME_APPS_LIST",
      "locators": {
        "android": {
          "selector": "rvApp",
          "strategy": "id",
          "case_insensitive": false
        },
        "ios": {
          "selector": "type == 'XCUIElementTypeCollectionView'",
          "strategy": "ios_predicate",
          "case_insensitive": false
        }
      }
    },
    {
      "name": "HOME_APP_BUY_CARD_BUTTON",
      "locators": {
        "android": {
          "selector": "Thẻ ĐT",
          "strategy": "text",
          "case_insensitive": false
        }
      }
    },
    {
      "name": "HOME_APP_CASH_IN_BUTTON",
      "locators": {
        "android": {
          "selector": "Nạp Tiền",
          "strategy": "text",
          "case_insensitive": false
        },
        "ios": {
          "selector": "label == \"Nạp Tiền\"",
          "strategy": "ios_predicate",
          "case_insensitive": false
        }
      }
    },
    {
      "name": "HOME_APP_LINK_BANK_BUTTON",
      "locators": {
        "android": {
          "selector": "Liên Kết Ngân Hàng",
          "strategy": "text",
          "case_insensitive": false
        },
        "ios": {
          "selector": "label ==[c] 'Liên Kết Ngân Hàng'",
          "strategy": "ios_predicate",
          "case_insensitive": false
        }
      }
    },
    {
      "name": "HOME_APP_LIXI_BUTTON",
      "locators": {
        "android": {
          "selector": "Lì Xì",
          "strategy": "text",
          "case_insensitive": false
        }
      }
    },
    {
      "name": "HOME_APP_MOBILE_TOPUP_BUTTON",
      "locators": {
        "android": {
          "selector": "Nạp ĐT",
          "strategy": "text",
          "case_insensitive": false
        },
        "ios": {
          "selector": "label == \"Nạp ĐT\"",
          "strategy": "ios_predicate",
          "case_insensitive": false
        }
      }
    },
    {
      "name": "SB_HOME_APP_MOBILE_TOPUP_BUTTON",
      "locators": {
        "android": {
          "selector": "Nạp ĐT",
          "strategy": "text",
          "case_insensitive": false
        }
      }
    },
    {
      "name": "HOME_APP_TRANSFER_BUTTON",
      "locators": {
        "android": {
          "selector": "Chuyển Tiền",
          "strategy": "text",
          "case_insensitive": false
        },
        "ios": {
          "selector": "label == \"Chuyển Tiền\"",
          "strategy": "ios_predicate",
          "case_insensitive": false
        }
      }
    },
    {
      "name": "HOME_APP_WITHDRAW_BUTTON",
      "locators": {
        "android": {
          "selector": "Rút Tiền",
          "strategy": "text",
          "case_insensitive": false
        },
        "ios": {
          "selector": "label == \"Rút Tiền\"",
          "strategy": "ios_predicate",
          "case_insensitive": false
        }
      }
    },
    {
      "name": "HOME_BALANCE_LABEL",
      "locators": {
        "android": {
          "selector": "new UiSelector().resourceIdMatches(\".*header_app_container\").instance(0).childSelector(new UiSelector().className(\"android.widget.TextView\").instance(2));",
          "strategy": "android_ui_automator",
          "case_insensitive": false
        },
        "ios": {
          "selector": "type == 'XCUIElementTypeStaticText' AND label ENDSWITH[c] 'Đ'",
          "strategy": "ios_predicate",
          "case_insensitive": false
        }
      }
    },
    {
      "name": "HOME_NEW_NOTIFICATION_LABEL",
      "locators": {
        "android": {
          "selector": "txtNotificationCount",
          "strategy": "id",
          "case_insensitive": false
        }
      }
    },
    {
      "name": "HOME_NOTIFICATION_BUTTON",
      "locators": {
        "android": {
          "selector": "imgNotification",
          "strategy": "id",
          "case_insensitive": false
        }
      }
    },
    {
      "name": "HOME_NOTIFICATION_ICON",
      "locators": {
        "android": {
          "selector": "notification_container",
          "strategy": "id",
          "case_insensitive": false
        },
        "ios": {
          "selector": "**/XCUIElementTypeNavigationBar[`name == \"ZaloPay.ZPHomeView\"`]/XCUIElementTypeButton[2]",
          "strategy": "ios_class_chain",
          "case_insensitive": false
        }
      }
    },
    {
      "name": "HOME_PROFILE_BUTTON",
      "locators": {
        "android": {
          "selector": "menu_profile",
          "strategy": "id",
          "case_insensitive": false
        },
        "ios": {
          "selector": "//XCUIElementTypeButton[@name='Cá Nhân']",
          "strategy": "xpath",
          "case_insensitive": false
        }
      }
    },
    {
      "name": "HOME_PROMOTION_LIST",
      "locators": {
        "android": {
          "selector": "rvPromotion",
          "strategy": "id",
          "case_insensitive": false
        }
      }
    },
    {
      "name": "HOME_TRANSACTION_HISTORY_BUTTON",
      "locators": {
        "android": {
          "selector": "menu_transaction",
          "strategy": "id",
          "case_insensitive": false
        }
      }
    },
    {
      "name": "HOME_APP_LUCKY_MONEY_BUTTON",
      "locators": {
        "android": {
          "selector": "Lì Xì",
          "strategy": "text",
          "case_insensitive": false
        },
        "ios": {
          "selector": "label == \"Lì Xì\"",
          "strategy": "ios_predicate",
          "case_insensitive": false
        }
      }
    }
  ]
}